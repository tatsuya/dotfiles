[user]
	name = Tatsuya Oiwa
[alias]
	st = status
	addu = add -u
	co = checkout
	br = branch
	mg = merge
	# http://stackoverflow.com/a/9074343/1769462
	lg1 = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'
	lg2 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
	lg = !"git lg1"
	logg = !"git lg1"
	fe = fetch --prune
	rb = rebase
	ci = commit
	df = diff
	dfc = diff --cached
	undo = reset --hard
	unstage = reset HEAD
[color]
	ui = auto
[core]
	editor = vim -c \"set fenc=utf-8\"
[credential]
	helper = cache --timeout 21600
